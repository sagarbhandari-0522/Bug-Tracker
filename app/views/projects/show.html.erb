<% if @project.present?%>
<h1> <%= @project.title %></h1>
<p> <%= @project.description%></p>
<p> Bugs  </p>
<% @project.bugs.all.each do |bug|%>
<strong><%=bug.title%></strong>
<%= bug.body%>
<%= bug.status%>
<%= link_to 'Assign', assign_path(bug) %><br>
<%end%>
<%else%>
Project Not Found
<% end %>
<%= link_to 'Back',project_manager_path(current_user) %>
<%= link_to 'Edit',edit_project_path(@project)%>
<%= button_to 'Delete Project', @project , method: :delete %>
<%= link_to 'Back To Home', project_manager_path(current_user)%>
